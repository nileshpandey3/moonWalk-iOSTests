{
  "presets": [
    "module:metro-react-native-babel-preset"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "module-resolver",
      {
        "root": "./src",
        "alias": {
          "src": "./src",
          "common": "./src/Common",
          "components": "./src/Components",
          "models": "./src/Models"
        }
      }
    ]
  ]
}